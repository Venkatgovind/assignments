import java.util.*;

class EvenOdd

 {

    public static void main(String[] args) 

    {

      int n,m;

      Scanner sc= new Scanner(System.in);

      System.out.print("enter the n value:");

      n=sc.nextInt();

      System.out.print("enter the m value:");

      m=sc.nextInt();

    System.out.print("List of numbers:");

     while(n<=m)

{

     System.out.println(m);

       m=m-2;

}

    }

}
